{
  "executable": "BibleNote.Application",
  "splashscreen": {
    "imageFile": "/wwwroot/assets/splashscreen.png"
  },
  "name": "BibleNote",
  "author": "",
  "singleInstance": true,
  "aspCoreBackendPort": 8079,
  "environment": "Production",
  "build": {
    "appId": "com.BibleNote.app",
    "productName": "BibleNote",
    "copyright": "Copyright Â© 2011-2020 Sasha and Natasha",
    "buildVersion": "4.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ],
    "win": {
      "icon": "bin/wwwroot/icon.png"
    }
  }
}